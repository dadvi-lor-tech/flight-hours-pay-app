<?xml version='1.0' encoding='UTF-8'?>
<domain-models xmlns='http://axelor.com/xml/ns/domain-models' xmlns:xsi='http://www.w3.org/2001/XMLSchema-instance'
   xsi:schemaLocation='http://axelor.com/xml/ns/domain-models http://axelor.com/xml/ns/domain-models/domain-models_5.2.xsd'>

   <module name="flight" package="com.axelor.apps.flight.db" />

   <entity name="ActualDuty" jsonAttrs="false">

      <date name="departureDate" title="Departure date" />
      <many-to-one name="duty" title="Duty" ref="com.axelor.apps.flight.db.Duty" />

      <integer name="actualDuration" title="Actual duration" />
      <integer name="durationDifference" title="Duration difference" />

      <boolean name="isWoff" title="WOFF" />
      <boolean name="isOob" title="Out Of Base" />

   </entity>

</domain-models>
